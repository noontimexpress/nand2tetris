/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=b, b=sel, out=bAndSel);
    Not(in=sel, out=invSel);
    And(a=a, b=invSel, out=aAndInvSel);
    Or(a=aAndInvSel, b=bAndSel, out=out);
}